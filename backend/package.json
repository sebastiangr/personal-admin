{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "@prisma/client": "^6.16.3",
    "test": "dotenv -e ../.env.test -- vitest run",
    "seed": "ts-node prisma/seed.ts",
    "start": "node dist/index.js",
    "dev": "dotenv -e ../.env -- ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "prisma:migrate": "dotenv -e ../.env -- npx prisma migrate dev",
    "prisma:studio": "dotenv -e ../.env -- npx prisma studio",
    "prisma:generate": "dotenv -e ../.env -- npx prisma generate",
    "db:seed": "dotenv -e ../.env -- npx prisma db seed",
    "db:seed:direct": "dotenv -e ../.env -- ts-node prisma/seed.ts",
    "prisma:reset": "dotenv -e ../.env -- npx prisma migrate reset"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@types/body-parser": "^1.19.6",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.6.2",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^10.0.0",
    "prisma": "^6.16.3",
    "supertest": "^7.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
